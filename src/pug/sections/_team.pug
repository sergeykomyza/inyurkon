section.team 
  
  .team__box
    .team__scrollbox 
      .team__scroll
        each item in team
            .team__inner
              h2.line-title= item.category
              .team__items 
                each elem in item.items 
                  a.preview-persona(href="#") 
                    img.preview-persona__foto(src="img/" + elem.foto, alt="@")
                    .preview-persona__fio= elem.fio
                    .preview-persona__status= elem.status